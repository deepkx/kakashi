data<-read.csv(file.choose(),header=T)

d1<-c(data$D1[1:3])
d2<-c(data$D2[1:3])
d3<-c(data$D3[1:3])
print(d1)
print(d2)
print(d3)
xx<-matrix(c(d1,d2,d3),nrow=3,ncol=3,byrow=F)
print(xx)
demand<-c(data$D1[4],data$D2[4],data$D3[4])
print(demand)
supply<-c(data$Supply[1:3])
print(supply)
row<-c('=','=','=')
col<-c('=','=','=')
tran<-lp.transport(xx,direction="min",row,supply,col,demand)
print(tran)
